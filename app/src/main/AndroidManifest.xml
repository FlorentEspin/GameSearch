<?xml version="1.0" encoding="utf-8"?>
<manifest xmlns:android="http://schemas.android.com/apk/res/android"
    package="gameproject.gamesearch" >

    <uses-permission android:name="android.permission.INTERNET" />
    <uses-permission android:name="android.permission.READ_LOGS" />

    <application
        android:allowBackup="true"
        android:icon="@mipmap/ic_launcher"
        android:label="@string/app_name"
        android:theme="@style/AppTheme" >
        <activity
            android:name=".PrimaryActivity"
            android:label="@string/title_activity_primary" >
            <intent-filter>
                <action android:name="android.intent.action.StartScreen" />

                <category android:name="android.intent.category.LAUNCHER" />
            </intent-filter>
        </activity>
        <activity
            android:name=".StartScreen"
            android:label="@string/title_activity_start_screen" >
            <intent-filter>
                <action android:name="android.intent.action.MAIN" />

                <category android:name="android.intent.category.LAUNCHER" />
            </intent-filter>
        </activity>
        <activity
            android:name=".Liste_jeux"
            android:label="@string/title_activity_liste_jeux" >
        </activity>
        <activity
            android:name=".Liste_utilisateurs"
            android:label="@string/title_activity_liste_utilisateurs" >
        </activity>
    </application>

</manifest>
